                <p><%= command.description %></p>
                <% if (command.aliases) { %>
                    <p>Aliases: <code><%= command.aliases %></code></p>
                <% } %>
                <% if (command.subcommands) { %>
                    <% command.subcommands.forEach(subcommand => { %>
                        <h4><%= subcommand.name %></h4>
                        <p><%= subcommand.description %></p>
                        <p>Usage: <code><%= prefix %><%= subcommand.usage %></code></p>
                    <% }) %>
                <% } else { %>
