                    <% }) %>
                <% } else { %>
                    <p>Usage: <code><%= prefix %><%= command.usage %></code></p>
                <% } %>
            <% }) %>
        </div>

        <%- include('./partials/scripts.ejs') %>
    </body>
</html>
